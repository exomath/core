{
  "name": "@exomath/core",
  "version": "0.17.1",
  "description": "Exomath core library",
  "repository": "https://github.com/exomath/core",
  "author": "Jake McKown <jbmckown@gmail.com>",
  "license": "MIT",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    ".eslintrc.json",
    "tsconfig.json"
  ],
  "engines": {
    "node": ">=14",
    "npm": "use-yarn",
    "yarn": ">=1.22"
  },
  "scripts": {
    "build:ts": "rimraf lib && tsc",
    "build:js": "rimraf dist && rollup --config",
    "build": "yarn lint && yarn build:ts && yarn build:js",
    "lint": "eslint src/**",
    "test": "nyc --reporter=lcov --reporter=text --reporter=text-summary --require esm mocha lib/**/*.test.js",
    "repl": "sirv dist --port 3000"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-typescript2": "^0.30.0",
